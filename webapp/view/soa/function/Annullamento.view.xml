<mvc:View
	controllerName="rgssoa.controller.soa.function.Annullamento"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
	xmlns:f="sap.f"
>
	<f:DynamicPage
		showFooter="true"
		class="sapUiNoContentPadding"
	>
		<f:header>
			<f:DynamicPageHeader
				class="noPadding"
				pinnable="false"
			>
				<c:Fragment
					fragmentName="rgssoa.view.fragment.panel.IconTabBarSoa"
					type="XML"
				/>
			</f:DynamicPageHeader>
		</f:header>
		<f:content>
			<c:Fragment
				fragmentName="rgssoa.view.fragment.table.TableSoaFunction"
				type="XML"
			/>
		</f:content>
		<f:footer>
			<OverflowToolbar>
				<Button
					type="Emphasized"
					text="{i18n>btnBack}"
					press="onNavBack"
				>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</Button>
				<ToolbarSpacer/>
				<Button
					type="Emphasized"
					text="{i18n>btnAnnullaSoa}"
					press="onAnnulla"
				>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</Button>
			</OverflowToolbar>
		</f:footer>
	</f:DynamicPage>
</mvc:View>