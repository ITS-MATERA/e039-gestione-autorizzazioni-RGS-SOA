<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
>
	<NavContainer>
		<pages>
			<Page showHeader="false">
				<content>
					<c:Fragment
						fragmentName="rgssoa.view.fragment.panel.IconTabBarSoa"
						type="XML"
					/>
					<Panel
						class="sapUiNoContentPadding"
						backgroundDesign="Transparent"
					>
						<Wizard
							id="wizScenario1"
							showNextButton="false"
						>
							<WizardStep title="{i18n>titleWizard1DocContabili}">
								<c:Fragment
									fragmentName="rgssoa.view.fragment.panel.FiltersDocumenti"
									type="XML"
								/>
								<c:Fragment
									fragmentName="rgssoa.view.fragment.table.scenario1.TableAddQuoteDocumenti"
									type="XML"
								/>
								<c:Fragment
									fragmentName="rgssoa.view.fragment.table.scenario1.TableEditQuoteDocumentiScen1"
									type="XML"
								/>
								<c:Fragment
									fragmentName="rgssoa.view.fragment.table.scenario1.TableRiepQuoteDocumentiScen1"
									type="XML"
								/>
							</WizardStep>
							<WizardStep title="{i18n>titleWizard2}">
								<c:Fragment
									fragmentName="rgssoa.view.fragment.soa.create.wizard2.BeneficiarioSoa"
									type="XML"
								/>
							</WizardStep>
							<WizardStep title="{i18n>titleWizard3}">
								<c:Fragment
									fragmentName="rgssoa.view.fragment.soa.create.wizard3.Classificazione"
									type="XML"
								/>
							</WizardStep>
							<WizardStep title="{i18n>titleWizard4}">
								<c:Fragment
									fragmentName="rgssoa.view.fragment.soa.create.wizard4.DatiSoa"
									type="XML"
								/>
							</WizardStep>
						</Wizard>
					</Panel>
				</content>
			</Page>
		</pages>
	</NavContainer>
</c:FragmentDefinition>