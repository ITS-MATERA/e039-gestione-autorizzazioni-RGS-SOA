<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
	xmlns:l="sap.ui.layout"
 	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
	<Panel backgroundDesign="Transparent">
		<headerToolbar>
			<OverflowToolbar>
				<Title text="{i18n>titleDatiVaglia}" />
			</OverflowToolbar>
		</headerToolbar>

		<content>
			<l:Grid defaultSpan="XL12 L12 M12 S12" hSpacing="0.5">
				<Text text="{i18n>inputTipoFirma}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
					<layoutData><l:GridData span="XL1 L3 M5 S12"/></layoutData>
				</Text>
				<Input editable="false">
					<layoutData><l:GridData span="XL2 L3 M5 S12"/></layoutData>
				</Input>

				<Text text="{i18n>inputNome}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
					<layoutData><l:GridData span="XL1 L3 M5 S12" linebreak="true"/></layoutData>
				</Text>
				<Input editable="false" value="{= ${Soa>/Zwels} === 'ID1' ? ${Soa>/ZpersNomeQuiet1} : ${Soa>/ZpersNomeVaglia} }">
					<layoutData><l:GridData span="XL2 L3 M5 S12"/></layoutData>
				</Input>
				<Text text="{i18n>inputCognome}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
					<layoutData><l:GridData span="XL1 L3 M5 S12"/></layoutData>
				</Text>
				<Input 
					editable="false" 
					value="{= ${Soa>/Zwels} === 'ID1' ? ${Soa>/ZpersCognomeQuiet1} : ${Soa>/ZpersCognomeVaglia} }">
					<layoutData><l:GridData span="XL2 L3 M5 S12"/></layoutData>
				</Input>
				<Text text="{i18n>inputCF}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
					<layoutData><l:GridData span="XL1 L3 M5 S12"/></layoutData>
				</Text>
				<Input editable="false">
					<layoutData><l:GridData span="XL2 L3 M5 S12"/></layoutData>
				</Input>

				<Text text="{i18n>inputIndirizzo}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
					<layoutData><l:GridData span="XL1 L3 M5 S12" linebreak="true"/></layoutData>
				</Text>
				<Input 
					editable="false"
					value="{Soa>/Zqindiriz}"	
				>
					<layoutData><l:GridData span="XL2 L3 M5 S12"/></layoutData>
				</Input>
				<Text text="{i18n>inputCitta}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
					<layoutData><l:GridData span="XL1 L3 M5 S12" /></layoutData>
				</Text>
				<Input 
					editable="false"
					value="{Soa>/Zqcitta}"
				>
					<layoutData><l:GridData span="XL2 L3 M5 S12"/></layoutData>
				</Input>
				<Text text="{i18n>inputCap}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
					<layoutData><l:GridData span="XL1 L3 M5 S12"/></layoutData>
				</Text>
				<Input 
					editable="false"
					value="{Soa>/Zqcap}"
				>
					<layoutData><l:GridData span="XL2 L3 M5 S12"/></layoutData>
				</Input>
				<Text text="{i18n>inputPaeseResidenza}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
					<layoutData><l:GridData span="XL2 L3 M5 S12"/></layoutData>
				</Text>
				<Input 
					editable="false"
					value="{Soa>/Zqprovincia}"
				>
					<layoutData><l:GridData span="XL1 L3 M5 S12"/></layoutData>
				</Input>

				<Text text="{i18n>inputNome}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
					<layoutData><l:GridData span="XL1 L3 M5 S12" linebreak="true"/></layoutData>
				</Text>
				<Input 
					editable="false"
					value="{Soa>/ZpersNomeQuiet2}"

				>
					<layoutData><l:GridData span="XL2 L3 M5 S12"/></layoutData>
				</Input>
				<Text text="{i18n>inputCognome}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
					<layoutData><l:GridData span="XL1 L3 M5 S12"/></layoutData>
				</Text>
				<Input 
					editable="false"
					value="{Soa>/ZpersCognomeQuiet2}"
				>
					<layoutData><l:GridData span="XL2 L3 M5 S12"/></layoutData>
				</Input>
				<Text text="{i18n>inputCF}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
					<layoutData><l:GridData span="XL1 L3 M5 S12"/></layoutData>
				</Text>
				<Input editable="false">
					<layoutData><l:GridData span="XL2 L3 M5 S12"/></layoutData>
				</Input>

				<Text text="{i18n>inputIndirizzo}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
					<layoutData><l:GridData span="XL1 L3 M5 S12" linebreak="true"/></layoutData>
				</Text>
				<Input 
					editable="false"
					value="{Soa>/Zqindiriz12}"
				>
					<layoutData><l:GridData span="XL2 L3 M5 S12"/></layoutData>
				</Input>
				<Text text="{i18n>inputCitta}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
					<layoutData><l:GridData span="XL1 L3 M5 S12" /></layoutData>
				</Text>
				<Input 
					editable="false"
					value="{Soa>/Zqcitta12}"
				>
					<layoutData><l:GridData span="XL2 L3 M5 S12"/></layoutData>
				</Input>
				<Text text="{i18n>inputCap}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
					<layoutData><l:GridData span="XL1 L3 M5 S12"/></layoutData>
				</Text>
				<Input 
					editable="false"
					value="{Soa>/Zqcap12}"
				>
					<layoutData><l:GridData span="XL2 L3 M5 S12"/></layoutData>
				</Input>
				<Text text="{i18n>inputPaeseResidenza}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
					<layoutData><l:GridData span="XL2 L3 M5 S12"/></layoutData>
				</Text>
				<Input 
					editable="false"
					value="{Soa>/Zqprovincia12}"
				>
					<layoutData><l:GridData span="XL1 L3 M5 S12"/></layoutData>
				</Input>
			</l:Grid>
		</content>

	</Panel>
</c:FragmentDefinition>