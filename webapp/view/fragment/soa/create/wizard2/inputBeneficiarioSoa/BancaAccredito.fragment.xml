<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
	xmlns:l="sap.ui.layout"
>
	<Panel
		backgroundDesign="Transparent"
		visible="{= ${Soa>/Zwels} === 'ID6' || ${Soa>/Zwels} === 'ID10' ? true : false}"
	>
		<headerToolbar>
			<OverflowToolbar>
				<Title text="{i18n>titleBancaAccredito}"/>
			</OverflowToolbar>
		</headerToolbar>
		<content>
			<l:Grid
				defaultSpan="XL12 L12 M12 S12"
				hSpacing="0.5"
			>
				<Text
					text="{i18n>inputIban}"
					class="sapUiTinyMarginTop sapUiLargeMarginEnd"
				>
					<layoutData>
						<l:GridData span="XL2 L3 M5 S12"/>
					</layoutData>
				</Text>
				<Input
					editable="false"
					value="{Soa>/Zibanb}"
					maxLength="34"
				>
					<layoutData>
						<l:GridData span="XL2 L3 M5 S12"/>
					</layoutData>
				</Input>
				<Text
					text="{i18n>inputBic}"
					class="sapUiTinyMarginTop sapUiLargeMarginEnd"
				>
					<layoutData>
						<l:GridData span="XL2 L3 M5 S12"/>
					</layoutData>
				</Text>
				<Input
					editable="false"
					value="{Soa>/Zbicb}"
					maxLength="11"
				>
					<layoutData>
						<l:GridData span="XL2 L3 M5 S12"/>
					</layoutData>
				</Input>
				<Text
					text="{i18n>inputCoordinateEstere}"
					class="sapUiTinyMarginTop sapUiLargeMarginEnd"
				>
					<layoutData>
						<l:GridData span="XL2 L3 M5 S12"/>
					</layoutData>
				</Text>
				<Input
					editable="false"
					value="{Soa>/Zcoordestb}"
					maxLength="35"
				>
					<layoutData>
						<l:GridData span="XL2 L3 M5 S12"/>
					</layoutData>
				</Input>
				<Title text="{i18n>titleDatiAltraBanca}"/>
				<Text
					text="{i18n>inputDenBanca}"
					class="sapUiTinyMarginTop sapUiLargeMarginEnd"
				>
					<layoutData>
						<l:GridData
							span="XL2 L3 M5 S12"
							linebreak="true"
						/>
					</layoutData>
				</Text>
				<Input
					editable="false"
					value="{Soa>/Zdenbancab}"
					maxLength="70"
				>
					<layoutData>
						<l:GridData span="XL2 L3 M5 S12"/>
					</layoutData>
				</Input>
				<Text
					text="{i18n>inputClearingSystemid}"
					class="sapUiTinyMarginTop sapUiLargeMarginEnd"
				>
					<layoutData>
						<l:GridData span="XL2 L3 M5 S12"/>
					</layoutData>
				</Text>
				<Input
					editable="false"
					value="{Soa>/Zclearsystb}"
					maxLength="5"
				>
					<layoutData>
						<l:GridData span="XL2 L3 M5 S12"/>
					</layoutData>
				</Input>
				<Title text="{i18n>titleIndirizzoAltraBanca}"/>
				<Text
					text="{i18n>inputVia}"
					class="sapUiTinyMarginTop sapUiLargeMarginEnd"
				>
					<layoutData>
						<l:GridData
							span="XL1 L3 M5 S12"
							linebreak="true"
						/>
					</layoutData>
				</Text>
				<Input
					editable="false"
					value="{Soa>/Strasb}"
					maxLength="35"
				>
					<layoutData>
						<l:GridData span="XL2 L3 M5 S12"/>
					</layoutData>
				</Input>
				<Text
					text="{i18n>inputCivico}"
					class="sapUiTinyMarginTop sapUiLargeMarginEnd"
				>
					<layoutData>
						<l:GridData span="XL1 L3 M5 S12"/>
					</layoutData>
				</Text>
				<Input
					editable="false"
					value="{Soa>/Zcivicob}"
					maxLength="5"
				>
					<layoutData>
						<l:GridData span="XL2 L3 M5 S12"/>
					</layoutData>
				</Input>
				<Text
					text="{i18n>inputCitta}"
					class="sapUiTinyMarginTop sapUiLargeMarginEnd"
				>
					<layoutData>
						<l:GridData
							span="XL1 L3 M5 S12"
							linebreak="true"
						/>
					</layoutData>
				</Text>
				<Input
					editable="false"
					value="{Soa>/Ort01b}"
					maxLength="35"
				>
					<layoutData>
						<l:GridData span="XL2 L3 M5 S12"/>
					</layoutData>
				</Input>
				<Text
					text="{i18n>inputProvincia}"
					class="sapUiTinyMarginTop sapUiLargeMarginEnd"
				>
					<layoutData>
						<l:GridData span="XL1 L3 M5 S12"/>
					</layoutData>
				</Text>
				<Input
					editable="false"
					value="{Soa>/Regiob}"
					maxLength="3"
				>
					<layoutData>
						<l:GridData span="XL2 L3 M5 S12"/>
					</layoutData>
				</Input>
				<Text
					text="{i18n>inputCap}"
					class="sapUiTinyMarginTop sapUiLargeMarginEnd"
				>
					<layoutData>
						<l:GridData span="XL1 L3 M5 S12"/>
					</layoutData>
				</Text>
				<Input
					editable="false"
					value="{Soa>/Pstlzb}"
					maxLength="10"
				>
					<layoutData>
						<l:GridData span="XL2 L3 M5 S12"/>
					</layoutData>
				</Input>
				<Text
					text="{i18n>inputNazione}"
					class="sapUiTinyMarginTop sapUiLargeMarginEnd"
				>
					<layoutData>
						<l:GridData span="XL1 L3 M5 S12"/>
					</layoutData>
				</Text>
				<Input
					editable="false"
					value="{Soa>/Land1b}"
					maxLength="3"
				>
					<layoutData>
						<l:GridData span="XL2 L3 M5 S12"/>
					</layoutData>
				</Input>
			</l:Grid>
		</content>
	</Panel>
</c:FragmentDefinition>