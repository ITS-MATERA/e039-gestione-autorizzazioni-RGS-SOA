<core:FragmentDefinition
    id="valueHelpCodiceCPV"
    xmlns="sap.ui.comp.valuehelpdialog"
    xmlns:m="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core"
>
    <ValueHelpDialog
        title="{i18n>labelCpv}"
        ok="onValueHelpOkPress"
        cancel="onValueHelpCancelPress"
        afterClose="onValueHelpAfterClose"
        supportRanges="false"
        key="Zcpv"
        descriptionKey="ZcpvDesc"
        tokenDisplayBehaviour="idAndDescription"
        supportMultiselect="false"
    >
        <filterBar>
            <fb:FilterBar
                id="idFilterBar"
                search="onFilterBarSearch"
                advancedMode="true"
            >
                <fb:filterGroupItems>
                    <fb:FilterGroupItem
                        groupName="__$INTERNAL$"
                        name="Zcpv"
                        label="{i18n>labelCpv}"
                        visibleInFilterBar="true"
                    >
                        <fb:control>
                            <m:SearchField
                                id="Zcpv"
                                placeholder="{i18n>insertCpv}"
                                search="onFilterBarSearch"
                            />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        groupName="__$INTERNAL$"
                        name="ZcpvDesc"
                        label="{i18n>labelDescrizione}"
                        visibleInFilterBar="true"
                    >
                        <fb:control>
                            <m:SearchField
                                id="ZcpvDesc"
                                placeholder="{i18n>insertDescrizione}"
                                search="onFilterBarSearch"
                            />
                        </fb:control>
                    </fb:FilterGroupItem>
                </fb:filterGroupItems>
            </fb:FilterBar>
        </filterBar>
    </ValueHelpDialog>
</core:FragmentDefinition>
