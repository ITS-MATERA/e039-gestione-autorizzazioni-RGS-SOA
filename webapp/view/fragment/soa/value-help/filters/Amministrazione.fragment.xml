<c:FragmentDefinition
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
    <SelectDialog
        id="sdAmministrazione"
        noDataText="{i18n>noData}"
        title="{i18n>labelAmministrazione}"
        search="onValueHelpChange"
        confirm="onValueHelpAmministrazioneClose"
        cancel="onValueHelpAmministrazioneClose"
        app:FilterValueHelp="Zzamministr"
        items="{
          path: 'Amministrazione>/'
		    }"
    >
        <StandardListItem
            iconDensityAware="false"
            iconInset="false"
            title="{Amministrazione>Zzamministr}"
        />
    </SelectDialog>
</c:FragmentDefinition>
