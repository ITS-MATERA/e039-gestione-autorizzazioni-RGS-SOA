<c:FragmentDefinition
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
    xmlns:l="sap.ui.layout"
    xmlns:zcontrolmccross="acn.zmatchcodecross.controls"
>
    <Panel backgroundDesign="Transparent">
        <headerToolbar>
            <OverflowToolbar>
                <Title text="{i18n>searchBeneficiario}" />
            </OverflowToolbar>
        </headerToolbar>
        <content>
            <l:Grid
                defaultSpan="XL12 L12 M12 S12"
                hSpacing="0.5"
            >
                <Text
                    text="{i18n>BeneficiarioRettificaFilterTipoBen}"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Text>
                <ComboBox
                    width="100%"
                    items="{
                        path: '/TipologiaBeneficiarioSet'
                    }"
                    selectedKey="{BeneficiarioRettifica>/Type}"
                    editable="false"
                >
                    <c:Item
                        key="{CodTipoBen}"
                        text="{DescTipoBen}"
                    />
                    <layoutData>
                        <l:GridData span="XL2 L2 M5 S12" />
                    </layoutData>
                </ComboBox>
                <Text
                    text="{i18n>filterBeneficiario}"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Text>
                <Input
                    value="{BeneficiarioRettifica>/Lifnr}"
                    editable="false"
                >
                    <layoutData>
                        <l:GridData span="XL2 L2 M6 S12" />
                    </layoutData>
                </Input>
            </l:Grid>
        </content>
        <content>
            <l:Grid
                defaultSpan="XL12 L12 M12 S12"
                hSpacing="0.5"
            >
                <Text
                    text="{i18n>BeneficiarioRettificaFilterName}"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Text>
                <Input
                    value="{BeneficiarioRettifica>/Znomebensosp}"
                    editable="false"
                >
                    <layoutData>
                        <l:GridData span="XL2 L2 M5 S12" />
                    </layoutData>
                </Input>
                <Text
                    text="{i18n>BeneficiarioRettificaFilterSurname}"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Text>
                <Input
                    value="{BeneficiarioRettifica>/Zcognomebensosp}"
                    editable="false"
                >
                    <layoutData>
                        <l:GridData span="XL2 L2 M5 S12" />
                    </layoutData>
                </Input>
                <Text
                    text="{i18n>BeneficiarioRettificaFilterRagSociale}"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Text>
                <Input
                    value="{BeneficiarioRettifica>/Zragsocbensosp}"
                    editable="false"
                >
                    <layoutData>
                        <l:GridData span="XL2 L2 M5 S12" />
                    </layoutData>
                </Input>
            </l:Grid>
        </content>
        <content>
            <l:Grid
                defaultSpan="XL12 L12 M12 S12"
                hSpacing="0.5"
            >
                <Text
                    text="{i18n>BeneficiarioRettificaFilterCF}"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Text>
                <Input
                    value="{BeneficiarioRettifica>/TaxnumCf}"
                    editable="false"
                >
                    <layoutData>
                        <l:GridData span="XL2 L2 M5 S12" />
                    </layoutData>
                </Input>
                <Text
                    text="{i18n>BeneficiarioRettificaFilterCFEstero}"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Text>
                <Input
                    value="{BeneficiarioRettifica>/Taxnumxl}"
                    editable="false"
                >
                    <layoutData>
                        <l:GridData span="XL2 L2 M5 S12" />
                    </layoutData>
                </Input>
                <Text
                    text="{i18n>BeneficiarioRettificaFilterPIVA}"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Text>
                <Input
                    value="{BeneficiarioRettifica>/TaxnumPiva}"
                    editable="false"
                >
                    <layoutData>
                        <l:GridData span="XL2 L2 M5 S12" />
                    </layoutData>
                </Input>
            </l:Grid>
        </content>
    </Panel>
</c:FragmentDefinition>
