<c:FragmentDefinition
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
    xmlns:l="sap.ui.layout"
    xmlns:zcontrolmccross="acn.zmatchcodecross.controls"
>
    <Panel>
        <content>
            <l:Grid
                defaultSpan="XL12 L12 M12 S12"
                hSpacing="0.5"
            >
                <Text
                    text="{i18n>inputNumeroProtocollo}"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M4 S12" />
                    </layoutData>
                </Text>
                <Input
                    value="{Soa>/Znumprot}"
                    maxLength="7"
                    type="Number"
                    editable="{Utility>/EnableEdit}"
                >
                    <layoutData>
                        <l:GridData span="XL1 L2 M2 S12" />
                    </layoutData>
                </Input>
                <Text
                    text="{i18n>inputDataProtocollo}"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData
                            span="XL2 L3 M4 S12"
                            indent="XL4"
                        />
                    </layoutData>
                </Text>
                <DatePicker
                    valueFormat="yyyy-MM-dd"
                    displayFormat="dd.MM.yyyy"
                    value="{
						path: 'Soa>/Zdataprot',
						type: 'sap.ui.model.type.Date',
						formatOptions: {
							pattern: 'dd.MM.yyyy'
						}
					}"
                    editable="{Utility>/EnableEdit}"
                    maxDate="{Utility>/CurrentDate}"
                >
                    <layoutData>
                        <l:GridData span="XL1 L2 M2 S12" />
                    </layoutData>
                </DatePicker>
                <Button
                    icon="sap-icon://documents"
                    width="100%"
                >
                    <layoutData>
                        <l:GridData
                            span="XL1 L2 M2 S12"
                            indentXL="1"
                        />
                    </layoutData>
                </Button>
                <Label
                    text="{i18n>inputLocalitaPagamento}"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                    required="true"
                >
                    <layoutData>
                        <l:GridData
                            span="XL2 L3 M4 S12"
                            linebreak="true"
                        />
                    </layoutData>
                </Label>
                <FlexBox class="flexBoxWitdh">
                    <items>
                        <zcontrolmccross:MatchcodeCross
                            id="iptLocalitaPagamento"
                            viewId="{Utility>/ViewId}"
                            whichMC="LocalitaPagamento"
                            value="{Soa>/Zlocpag}"
                            editable="{Utility>/EnableEdit}"
                            liveChange="onLocPagamentoChange"
                            maxLength="100"
                        />
                    </items>
                    <layoutData>
                        <l:GridData span="XL2 L2 M8 S12" />
                    </layoutData>
                </FlexBox>
                <Label
                    text="{i18n>inputCausalePagamento}"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                    required="true"
                >
                    <layoutData>
                        <l:GridData
                            span="XL2 L3 M4 S12"
                            linebreak="true"
                        />
                    </layoutData>
                </Label>
                <TextArea
                    maxLength="100"
                    value="{Soa>/Zcausale}"
                    width="100%"
                    cols="100"
                    editable="{Utility>/EnableEdit}"
                >
                    <layoutData>
                        <l:GridData span="XL8 L7 M8 S12" />
                    </layoutData>
                </TextArea>
                <Text
                    text="{i18n>inputE2EID}"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData
                            span="XL2 L3 M4 S12"
                            linebreak="true"
                        />
                    </layoutData>
                </Text>
                <TextArea
                    maxLength="35"
                    value="{Soa>/ZE2e}"
                    width="100%"
                    cols="100"
                    editable="{Utility>/EnableEdit}"
                >
                    <layoutData>
                        <l:GridData span="XL8 L7 M8 S12" />
                    </layoutData>
                </TextArea>
                <Text
                    text="{i18n>inputDataEsigibilita}"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData
                            span="XL2 L3 M4 S12"
                            linebreak="true"
                        />
                    </layoutData>
                </Text>
                <DatePicker
                    valueFormat="yyyy-MM-dd"
                    displayFormat="dd.MM.yyyy"
                    value="{
						path: 'Soa>/Zdataesig',
						type: 'sap.ui.model.type.Date',
						formatOptions: {
							pattern: 'dd.MM.yyyy'
						}
					}"
                    editable="{Utility>/EnableEdit}"
                >
                    <layoutData>
                        <l:GridData span="XL1 L2 M2 S12" />
                    </layoutData>
                </DatePicker>
                <Label
                    text="{i18n>inputZonaIntervento}"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                    required="true"
                >
                    <layoutData>
                        <l:GridData
                            span="XL2 L3 M4 S12"
                            indent="XL4"
                        />
                    </layoutData>
                </Label>
                <FlexBox class="flexBoxWitdh">
                    <items>
                        <zcontrolmccross:MatchcodeCross
                            id="iptZonaIntervento"
                            viewId="{Utility>/ViewId}"
                            whichMC="ZonaIntervento"
                            value="{Soa>/Zzonaint}"
                            editable="{Utility>/EnableEdit}"
                            valueHelpOnly="true"
                        />
                    </items>
                    <layoutData>
                        <l:GridData span="XL1 L2 M2 S12" />
                    </layoutData>
                </FlexBox>
            </l:Grid>
        </content>
    </Panel>
</c:FragmentDefinition>
