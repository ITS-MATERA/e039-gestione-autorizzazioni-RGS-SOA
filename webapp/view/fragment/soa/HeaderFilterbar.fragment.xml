<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
	xmlns:l="sap.ui.layout"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
	xmlns:zcontrol="zsap.com.r3.cobi.s4.custposfin.z_s4_zposfin.controls"
	xmlns:zcontrolMatchSar="zsap.com.r3.cobi.s4.custapiuser.z_s4_userlog.controls"
>
	<VBox>
		<l:Grid
			id="grdFilter"
			defaultSpan="XL12 L12 M12 S12"
			vSpacing="0"
			class="sapUiSmallMargin"
		>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>soaFilterGjahr}"
						class="filterLabel"
					/>
					<ComboBox
						width="100%"
						items="{
							path: '/EsercizioGestioneSet'
						}"
						selectedKey="{Filter>/Gjahr}"
					>
						<c:Item
							key="{Gjahr}"
							text="{Gjahr}"
						/>
					</ComboBox>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>soaFilterZzamministr}"
						class="filterLabel"
					/>
					<Input value="{Filter>/Zzamministr}"/>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>inputCapitolo}"
						class="filterLabel"
					/>
					<Input value="{Filter>/Zcapitolo}"/>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>soaFilterZnumsopFrom}"
						class="filterLabel"
					/>
					<Input value="{Filter>/ZnumsopFrom}"/>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>soaFilterZnumsopTo}"
						class="filterLabel"
					/>
					<Input value="{Filter>/ZnumsopTo}"/>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>soaFilterZcodStatosop}"
						class="filterLabel"
					/>
					<ComboBox
						width="100%"
						items="{
							path: '/StateSOASet'
						}"
						selectedKey="{Filter>/ZstatoSoa}"
					>
						<c:Item
							key="{ZstatoSoa}"
							text="{ZdescStatoSoa}"
						/>
					</ComboBox>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>soaFilterZchiaveaut}"
						class="filterLabel"
						required="true"
					/>
					<Input value="{Filter>/Zchiaveaut}"/>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>soaFilterZtipodisp2}"
						class="filterLabel"
					/>
					<ComboBox
						width="100%"
						selectedKey="{Filter>/Ztipodisp2}"
						selectionChange="onTipologiaAutorizzazioneChange"
						items="{
							path: '/TipoDisp2Set'
						}"
					>
						<c:Item
							key="{Ztipodisp2}"
							text="{Zdesctipodisp2}"
						/>
					</ComboBox>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>soaFilterZtipodisp3}"
						class="filterLabel"
					/>
					<ComboBox
						width="100%"
						selectedKey="{Filter>/Ztipodisp3}"
						items="{
							path: 'TipoDisp3Set>/'
						}"
					>
						<c:Item
							key="{TipoDisp3Set>Ztipodisp3}"
							text="{TipoDisp3Set>Zdesctipodisp3}"
						/>
					</ComboBox>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>soaFilterZztipologia}"
						class="filterLabel"
					/>
					<ComboBox
						width="100%"
						selectedKey="{Filter>/Zztipologia}"
						items="{
							path: '/TipoSOASet'
						}"
					>
						<c:Item
							key="{ZtipoSoa}"
							text="{ZdescTipoSoa}"
						/>
					</ComboBox>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>soaFilterDescTipopag}"
						class="filterLabel"
					/>
					<ComboBox
						width="100%"
						items="{
							path: '/TipoPagSet'
						}"
						selectedKey="{Filter>/DescTipopag}"
					>
						<c:Item
							key="{Low}"
							text="{Low}"
						/>
					</ComboBox>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>soaFilterZspecieSop}"
						class="filterLabel"
					/>
					<ComboBox
						width="100%"
						selectedKey="{Filter>/ZspecieSop}"
						items="{
							path: '/SpecieSOASet'
						}"
					>
						<c:Item
							key="{ZspecieSoa}"
							text="{ZdescSpecieSoa}"
						/>
					</ComboBox>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>soaFilterZricann}"
						class="filterLabel"
					/>
					<ComboBox
						width="100%"
						selectedKey="{Filter>/Zricann}"
					>
						<c:Item
							key="{i18n>itemNo}"
							text="{i18n>itemNo}"
						/>
						<c:Item
							key="{i18n>itemYes}"
							text="{i18n>itemYes}"
						/>
					</ComboBox>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>soaFilterZdatasopFrom}"
						class="filterLabel"
					/>
					<DatePicker
						valueFormat="yyyy-MM-dd"
						displayFormat="dd.MM.yyyy"
						value="{
							path: 'Filter>/ZdatasopFrom',
							type: 'sap.ui.model.type.Date',
							formatOptions: {
								pattern: 'dd.MM.yyyy'
							}
						}"
					/>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>soaFilterZdatasopTo}"
						class="filterLabel"
					/>
					<DatePicker
						valueFormat="yyyy-MM-dd"
						displayFormat="dd.MM.yyyy"
						value="{
							path: 'Filter>/ZdatasopTo',
							type: 'sap.ui.model.type.Date',
							formatOptions: {
								pattern: 'dd.MM.yyyy'
							}
						}"
					/>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>soaFilterZdataprot}"
						class="filterLabel"
					/>
					<DatePicker
						valueFormat="yyyy-MM-dd"
						displayFormat="dd.MM.yyyy"
						value="{
							path: 'Filter>/Zdataprot',
							type: 'sap.ui.model.type.Date',
							formatOptions: {
								pattern: 'dd.MM.yyyy'
							}
						}"
					/>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>soaFilterZnumprot}"
						class="filterLabel"
					/>
					<Input value="{Filter>/Znumprot}"/>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>filterBeneficiario}"
						class="filterLabel"
					/>
					<Input value="{Filter>/Lifnr}"/>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>soaFilterWitht}"
						class="filterLabel"
					/>
					<Input
						valueHelpRequest="onValueHelpRitenuta"
						showValueHelp="true"
						valueHelpOnly="true"
						app:key="{Filter>/Witht}"
						value="{Filter>/DescRitenuta}"
					/>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>soaFilterZzCebenra}"
						class="filterLabel"
					/>
					<Input
						valueHelpRequest="onValueHelpEnteBeneficiario"
						showValueHelp="true"
						valueHelpOnly="true"
						app:key="{Filter>/ZzCebenra}"
						value="{Filter>/DescEnte}"
					/>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData
						span="XL2 L3 M6 S12"
						linebreak="true"
					/>
				</layoutData>
				<items>
					<Label
						text="{i18n>soaFilterFiposFrom}"
						class="filterLabel"
					/>
					<zcontrol:InputPosizioneFinanziaria
						fase="GEST"
						spesa="X"
						reale="R"
						anno="{Filter>/Gjahr}"
						versione="D"
						maxSuggestionWidth="100%"
						width="100%"
						value="{Filter>/FiposFrom}"
						liveChange="onFiposFromChange"
					/>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>soaFilterFiposTo}"
						class="filterLabel"
					/>
					<zcontrol:InputPosizioneFinanziaria
						fase="GEST"
						spesa="X"
						reale="R"
						anno="{Filter>/Gjahr}"
						versione="D"
						maxSuggestionWidth="100%"
						width="100%"
						value="{Filter>/FiposTo}"
						liveChange="onFiposToChange"
					/>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL3 L4 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>soaFilterFistl}"
						class="filterLabel"
					/>
					<zcontrolMatchSar:GestioneLogUser
						eos="S"
						fase="GEST"
						entrata="X"
						reale="R"
						anno="{Filter>/Gjahr}"
						SEM_OBJ="Z_TEST_LIB"
						versione="D"
						width="100%"
						placeholder="Scegli struttura amministrativa"
						value="{Filter>/Fistl}"
						liveChange="onFistlChange"
					/>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData
						span="XL2 L3 M6 S12"
						linebreak="true"
					/>
				</layoutData>
				<items>
					<Label
						text="{i18n>filterZnumliqFrom}"
						class="filterLabel"
					/>
					<Input value="{Filter>/ZnumliqFrom}"/>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL2 L3 M6 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>filterZnumliqTo}"
						class="filterLabel"
					/>
					<Input value="{Filter>/ZnumliqTo}"/>
				</items>
			</VBox>
			<VBox>
				<layoutData>
					<l:GridData span="XL4 L6 M8 S12"/>
				</layoutData>
				<items>
					<Label
						text="{i18n>filterZdescProsp}"
						class="filterLabel"
					/>
					<Input value="{Filter>/ZdescProsp}"/>
				</items>
			</VBox>
			<Button
				id="btnSearchSoa"
				text="{i18n>btnStart}"
				tooltip="{i18n>btnStart}"
				class="button"
				width="100%"
				type="Emphasized"
				press="onSearch"
			>
				<layoutData>
					<l:GridData
						linebreak="true"
						span="XL1 L1 M1 S12"
						indent="XL11 L11 M11"
					/>
				</layoutData>
			</Button>
		</l:Grid>
	</VBox>
</c:FragmentDefinition>