<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
  xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
<SelectDialog
		id="sdIbanBeneficiario"
    noDataText="{i18n>noIbanBeneficiario}"
		title="{i18n>labelIbanBeneficiario}"
		search="onValueHelpChange"
		confirm="onValueHelpIbanClose"
		cancel="onValueHelpIbanClose"
		app:FilterValueHelp="Iban"
		items="{path: 'IbanBeneficiario>/'}">		
		<StandardListItem
      iconDensityAware="false"
      iconInset="false"
      title="{IbanBeneficiario>Iban}"/>
	</SelectDialog>
</c:FragmentDefinition>