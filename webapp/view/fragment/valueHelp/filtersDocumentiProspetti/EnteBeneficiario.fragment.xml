<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
  xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
<SelectDialog
		id="sdEnteBeneficiario"
    noDataText="{i18n>noEnteBeneficiario}"
		title="{i18n>columnNameEnteBeneficiario}"
		search="onValueHelpChange"
		confirm="onValueHelpEnteBeneficiarioClose"
		cancel="onValueHelpEnteBeneficiarioClose"
		app:FilterValueHelp="DescEnte"
		items="{path: 'EnteBeneficiario>/'}">		
		<StandardListItem
      iconDensityAware="false"
      iconInset="false"
      title="{EnteBeneficiario>DescEnte}"
      app:CodEnte="{EnteBeneficiario>CodEnte}"/>
	</SelectDialog>
</c:FragmentDefinition>