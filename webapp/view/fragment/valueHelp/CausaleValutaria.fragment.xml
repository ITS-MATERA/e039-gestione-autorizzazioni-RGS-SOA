<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
 xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
<SelectDialog
		id="sdCausaleValutaria"
    noDataText="{i18n>noCausaleValutaria}"
		title="{i18n>labelCausaleValutaria}"
		search="onValueHelpChange"
		confirm="onValueHelpCausaleValutariaClose"
		cancel="onValueHelpCausaleValutariaClose"
		app:FilterValueHelp="ZDesccauval"
		items="{path: 'CausaleValutaria>/'}">		
		<StandardListItem
      iconDensityAware="false"
      iconInset="false"
      title="{CausaleValutaria>ZDesccauval}"
      app:ZCausaleval="{CausaleValutaria>ZCausaleval}"/>
	</SelectDialog>
</c:FragmentDefinition>