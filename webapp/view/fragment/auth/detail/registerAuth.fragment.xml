<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:core="sap.ui.core"
  xmlns:zcontrolMatchSar="zsap.com.r3.cobi.s4.custapiuser.z_s4_userlog.controls"
  xmlns:zcontrol="zsap.com.r3.cobi.s4.custposfin.z_s4_zposfin.controls"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">

      <Panel>
        <l:Grid defaultSpan="XL12 L12 M12 S12" vSpacing="0">
            <Label text="{i18n>detailAuthEsercizioGestione}" class="sapUiSmallMarginEnd sapUiTinyMarginTop" wrapping="true" required="true" >
              <layoutData><l:GridData  span="XL1 L1 M6 S12" /></layoutData>
            </Label>  
            <ComboBox
							id="authfEsercizio"
              change="setPropertyComboFromChange"
              app:propertyModel="RegisterSet/Gjahr"
              selectedKey="{EntityModel>/RegisterSet/Gjahr}"
							width="100%"							
							items="{
								path:'/EsercizioGestioneSet'
							}">
							<core:Item key="{Gjahr}" text="{Gjahr}" />
							<layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
						</ComboBox>
        </l:Grid>
      </Panel>	

			<Panel  headerText="{i18n>authDetail-detail-datiAutorizzazioneRGS}">        
				<l:Grid defaultSpan="XL12 L12 M12 S12" vSpacing="0">
						<Label text="{i18n>detailAuthImportoAutorizzato}" class="sapUiSmallMarginEnd sapUiTinyMarginTop" wrapping="true" required="true">
              <layoutData><l:GridData  span="XL1 L1 M6 S12" /></layoutData>
            </Label>
						<Input value="{EntityModel>/RegisterSet/Zimpaut}" type="Number" >
						  <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
            </Input>
					
						<Label  text="{i18n>detailAuthTipologiaAutorizzazione}" class="sapUiSmallMarginEnd sapUiTinyMarginTop" wrapping="true" required="true">
              <layoutData><l:GridData  span="XL1 L1 M6 S12" linebreak="true"/></layoutData>
            </Label>
						<ComboBox
							id="detailAuthfTipologiaAutorizzazione"
							width="100%"													
							change="setPropertyComboFromChange"
              selectedKey="{EntityModel>/RegisterSet/Ztipodisp2}"
              app:propertyModel="RegisterSet/Ztipodisp2"
              app:propertyCallFunction="_loadTipologiaDisposizione"
							items="{
								path:'/TipoDisp2Set'
							}">
							<core:Item key="{Ztipodisp2}" text="{Zdesctipodisp2}" />
							<layoutData><l:GridData  span="XL3 L3 M6 S12" /></layoutData>
						</ComboBox>
						
						<Label  text="{i18n>detailAuthTipologiaDisposizione}" class="sapUiSmallMarginEnd sapUiTinyMarginTop" wrapping="true" required="true">
              <layoutData><l:GridData  span="XL1 L1 M6 S12" linebreak="true"/></layoutData>
            </Label>
						<ComboBox
							id="detailAuthfTipologiaDisposizione"
              change="setPropertyComboFromChange"
              selectedKey="{EntityModel>/RegisterSet/Ztipodisp3}"
              app:propertyModel="RegisterSet/Ztipodisp3"
							width="100%"							
							items="{
								path:'TipoDisp3Set>/'
							}">
							<core:Item key="{TipoDisp3Set>Ztipodisp3}" text="{TipoDisp3Set>Zdesctipodisp3}" />
							<layoutData><l:GridData  span="XL3 L3 M6 S12" /></layoutData>
						</ComboBox>
						

						<Label  text="{i18n>detailAuthNoteAutorizzazione}" class="sapUiSmallMarginEnd sapUiTinyMarginTop" wrapping="true">
              <layoutData><l:GridData  span="XL1 L1 M6 S12" linebreak="true"/></layoutData>
            </Label>
						<TextArea value="{EntityModel>/RegisterSet/Znoteaut}" cols="100" rows="5" maxLength="1000" width="100%">
              <layoutData><l:GridData  span="XL8 L8 M12 S12" /></layoutData>
            </TextArea>
						
						<Label text="{i18n>detailAuthUfficioIGEPA}" class="sapUiSmallMarginEnd sapUiTinyMarginTop" wrapping="true">
              <layoutData><l:GridData  span="XL1 L1 M6 S12" linebreak="true"/></layoutData>
            </Label>
						<Input width="100%" value="{EntityModel>/RegisterSet/ZufficioIgepa}" >
							<layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
						</Input>					

						<Label text="{i18n>detailAuthUfficioAmmRich}" class="sapUiSmallMarginEnd sapUiTinyMarginTop" wrapping="true">
              <layoutData><l:GridData  span="XL1 L1 M6 S12" /></layoutData>
            </Label>
						<Input width="100%" value="{EntityModel>/RegisterSet/ZufficioRich}" >
							<layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
						</Input>

						<Label text="{i18n>detailAuthUfficioUCBCompetente}" class="sapUiSmallMarginEnd sapUiTinyMarginTop" wrapping="true">
              <layoutData><l:GridData  span="XL1 L1 M6 S12" /></layoutData>
            </Label>
						<Input width="100%" value="{EntityModel>/RegisterSet/ZufficioUcb}" >
							<layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
						</Input>
				</l:Grid>
			</Panel>

			<Panel headerText="{i18n>authDetail-detail-imputazioneContabile}">
				<l:Grid defaultSpan="XL12 L12 M12 S12" vSpacing="0">
          <Label text="{i18n>detailAuthEsercizioFinanziario}"  class="sapUiSmallMarginEnd sapUiTinyMarginTop" wrapping="true" required="true">
            <layoutData><l:GridData  span="XL1 L1 M6 S12" /></layoutData>
          </Label>
          <ComboBox
            id="detailAuthfEsercizio"
            width="100%"
            selectedKey="{EntityModel>/RegisterSet/EsercicioFinanziario}"
            change="setPropertyComboFromChange"
            app:propertyModel="RegisterSet/EsercicioFinanziario"
            items="{
              path:'/EsercizioGestioneSet'
            }">
            <core:Item key="{Gjahr}" text="{Gjahr}" />
            <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
          </ComboBox>

          <Label text="{i18n>detailAuthPosizioneFinanziaria}" class="sapUiSmallMarginEnd sapUiTinyMarginTop" wrapping="true" required="true">
            <layoutData><l:GridData  span="XL1 L1 M6 S12" /></layoutData>
          </Label>
          <VBox>
          <zcontrol:InputPosizioneFinanziaria id="input_PF" fase="GEST" spesa="X" maxLength="24"
            reale="R" anno="{EntityModel>/RegisterSet/EsercicioFinanziario}" versione="D" maxSuggestionWidth="60%" submit="posizioneFinanziariaLiveChange" 
            liveChange="posizioneFinanziariaLiveChange"  width="100%" 
            app:propertyModel="RegisterSet/Fipos"
            value="{EntityModel>/RegisterSet/Fipos}"/>      
            <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
          </VBox>   
						
        	<Label text="{i18n>detailAuthStrutturaAmmResp}" class="sapUiSmallMarginEnd sapUiTinyMarginTop" wrapping="true">
            <layoutData><l:GridData  span="XL1 L1 M6 S12" linebreak="true" /></layoutData>
          </Label>
          <VBox>
           <zcontrolMatchSar:GestioneLogUser eos="S" id="struttAmmResp" fase="GEST" entrata="X" reale="R"  maxLength="16"
            anno="{EntityModel>/RegisterSet/EsercicioFinanziario}" SEM_OBJ="Z_TEST_LIB"  versione="D" width="100%" placeholder="Scegli struttura amministrativa"
            submit="strutturaAmministrativaLiveChange" liveChange="strutturaAmministrativaLiveChange"
            app:propertyModel="RegisterSet/Fistl"
            value="{EntityModel>/RegisterSet/Fistl}"/>
            <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
          </VBox>  
						
          <Label text="{i18n>detailAuthDescrizioneStrutturaAmmResp}" class="sapUiSmallMarginEnd sapUiTinyMarginTop" wrapping="true">
            <layoutData><l:GridData  span="XL1 L1 M6 S12" /></layoutData>
          </Label>
          <Input value="{EntityModel>/RegisterSet/Beschr}" width="100%" enabled="false">
            <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
          </Input>
					
					<CheckBox text="{i18n>detailAuthPosFinNonAncoraIstituita}"  
							selected="{EntityModel>/RegisterSet/ZflagFipos}" >
						<layoutData><l:GridData  span="XL3 L3 M6 S12" linebreak="true"/></layoutData>
					</CheckBox>
				</l:Grid>
			</Panel>

			<Panel headerText="{i18n>authDetail-detail-ufficio}">
				<l:Grid defaultSpan="XL12 L12 M12 S12" vSpacing="0">
          <Label text="{i18n>detailAuthUfficioOrdinante}" class="sapUiSmallMarginEnd sapUiTinyMarginTop" wrapping="true" required="true">
            <layoutData><l:GridData  span="XL1 L1 M6 S12" /></layoutData>
          </Label>
          <Input value="{EntityModel>/RegisterSet/ZufficioCont}" submit="checkZUfficioCont" width="100%" >
            <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
          </Input>
          <Label text="{i18n>detailAuthUfficioOrdinanteDescrizione}" class="sapUiSmallMarginEnd sapUiTinyMarginTop" wrapping="true">
            <layoutData><l:GridData  span="XL1 L1 M6 S12" /></layoutData>
          </Label>
          <Input value="{EntityModel>/RegisterSet/Descufficio}" width="100%" enabled="false">
            <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
          </Input>
						

          <Label text="{i18n>detailAuthCodiceFD}" class="sapUiSmallMarginEnd sapUiTinyMarginTop" wrapping="true">
              <layoutData><l:GridData  span="XL1 L1 M6 S12" linebreak="true"/></layoutData>
          </Label>
          <Input value="{EntityModel>/RegisterSet/Zfunzdel}" width="100%" enabled="false">
            <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
          </Input>						
          <Label text="{i18n>detailAuthCodiceFDDescrizione}" class="sapUiSmallMarginEnd sapUiTinyMarginTop" wrapping="true">
            <layoutData><l:GridData  span="XL1 L1 M6 S12" /></layoutData>
          </Label>
          <Input value="{EntityModel>/RegisterSet/Zdescriz}" width="100%" enabled="false">
            <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
          </Input>
						
				</l:Grid>
			</Panel>

			<Panel headerText="{i18n>authDetail-detail-datiProtocollo}">
				<l:Grid defaultSpan="XL12 L12 M12 S12" vSpacing="0">
					
          <Label text="{i18n>detailAuthNProtocolloAmm}" class="sapUiSmallMarginEnd">
            <layoutData><l:GridData  span="XL1 L1 M6 S12" /></layoutData>
          </Label>
          <Input value="{EntityModel>/RegisterSet/Znumprot}" width="100%" >
            <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
          </Input>
									
          <Label text="{i18n>detailAuthDataProtocolloAmm}" class="sapUiSmallMarginEnd">
            <layoutData><l:GridData  span="XL1 L1 M6 S12" /></layoutData>
          </Label>
          <DatePicker            
            change="setPropertyDatePickerFromChange"										
            valueFormat="dd/MM/yyyy"
            displayFormat="dd.MM.yyyy"
            app:propertyModel="RegisterSet/Zdataprot"
            >	
            <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
          </DatePicker>
				</l:Grid>
			</Panel>
</core:FragmentDefinition>