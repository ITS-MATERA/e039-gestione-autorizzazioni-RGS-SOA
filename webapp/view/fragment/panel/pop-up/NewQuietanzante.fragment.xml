<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
  xmlns:l="sap.ui.layout"
>
 <Panel 
      id="pnlNewQuietanzante"
      backgroundDesign="Transparent"
      visible="{NewAnagraficaBen>/VisibleNewQuietanzante}"
      >
    <l:Grid defaultSpan="XL12 L12 M12 S12" vSpacing="0.5">
      <Label 
        text="{i18n>inputCognome}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
        required="true"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Label>
      <Input
        value="{NewAnagraficaBen>/Quietanzante/Zqcognome}"
        maxLength="50"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>
      <Label 
        text="{i18n>inputNome}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
        required="true"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Label>
      <Input
        value="{NewAnagraficaBen>/Quietanzante/Zqnome}"
        maxLength="50"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>
      <Text 
        text="{i18n>inputCF}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Text>
      <Input
        value="{NewAnagraficaBen>/Quietanzante/Stcd1}"
        maxLength="16"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>
      <Text 
        text="{i18n>inputQualifica}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Text>
      <Input
        value="{NewAnagraficaBen>/Quietanzante/Zqqualifica}"
        maxLength="50"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>

      <Text 
        text="{i18n>inputDataNascita}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Text>
      <DatePicker
        width="100%"
        valueFormat="yyyy-MM-dd"
        value="{
                path: 'NewAnagraficaBen>/Quietanzante/Zqdatanasc',
                type: 'sap.ui.model.type.Date',
                formatOptions: {pattern: 'dd.MM.yyyy'}
              }"
        displayFormat="dd.MM.yyyy"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </DatePicker>
      <Text 
        text="{i18n>inputLuogoNascita}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Text>
      <Input
        value="{NewAnagraficaBen>/Quietanzante/Zqluogonasc}"
        maxLength="40"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>
      <Text 
        text="{i18n>inputProvinciaNascita}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"        
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Text>
      <Input
        value="{NewAnagraficaBen>/Quietanzante/Zqprovnasc}"
        maxLength="3"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>
      <Text 
        text="{i18n>inputTelefono}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Text>
      <Input 
        type="Number"
        value="{NewAnagraficaBen>/Quietanzante/Zqtelefono}"
        maxLength="20"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>

      <Text 
        text="{i18n>inputIndirizzo}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Text>
      <Input
        value="{NewAnagraficaBen>/Quietanzante/Zqindiriz}"
        maxLength="40"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>
      <Text 
        text="{i18n>inputCitta}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Text>
      <Input
        value="{NewAnagraficaBen>/Quietanzante/Zqcitta}"
        maxLength="40"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>
      <Text 
        text="{i18n>inputCap}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Text>
      <Input
        type="Number"
        value="{NewAnagraficaBen>/Quietanzante/Zqcap}"
        maxLength="10"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>
      <Text 
        text="{i18n>inputProvinciaResidenza}" 
        class="sapUiTinyMarginTop sapUiLargeMarginEnd"
      >
        <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
      </Text>
      <Input
        value="{NewAnagraficaBen>/Quietanzante/Zqprovincia}"
        maxLength="3"
      >
        <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
      </Input>
    </l:Grid>
  </Panel>
</c:FragmentDefinition>